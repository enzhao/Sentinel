# Environment variables for local development (dev mode)
# Copy this file to .env and fill in the values.
# The .env file is loaded automatically by the application in dev mode.

# Firebase Emulator Suite Configuration
# These are the default hosts and ports.
FIRESTORE_EMULATOR_HOST="localhost:8080"
FIREBASE_AUTH_EMULATOR_HOST="localhost:9099"
GCLOUD_PROJECT=  # Project ID used by emulators

# Alpha Vantage API Key for market data
ALPHA_VANTAGE_API_KEY="YOUR_API_KEY_HERE"